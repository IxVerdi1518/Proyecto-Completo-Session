@model Cliente
@{
    ViewData["Title"] = "Buscar por Cedula Post";
}
<div class="contairner">
    <h3>
        Buscar por Cedula
    </h3>
    <br />
</div>


<div class="contairner">
    <div class="row">
        <div>
              <form method="post" asp-controller="Cliente" asp-action="BuscarPorCedulaPost">
                <div class="form-group">
                    <label asp-for="Identificacion" class="control-label"></label>
                    <input asp-for="Identificacion" class="form-control" />
                </div>
                <div class="form-group">
                    <input type="submit" value="Guardar" class="btn btn-primary" />
                </div>
            </form>
        </div>
      
    </div>
</div>